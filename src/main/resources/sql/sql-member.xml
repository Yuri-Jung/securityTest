<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.securitytest.mapper.ClubMemberMapper">
    <select id="findByEmail" parameterType="map" resultType="com.example.securitytest.dto.ClubMemberDto">
        SELECT * FROM club_member
        WHERE email = #{email}
        AND from_social = #{social}
    </select>
</mapper>